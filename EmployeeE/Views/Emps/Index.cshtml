@model IEnumerable<EmployeeE.Models.Emp>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Ename)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Job)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Hiredate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Comm)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeptnoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MgrNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Ename)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Job)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Hiredate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Comm)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeptnoNavigation.Deptno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MgrNavigation.Empno)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Empno">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Empno">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Empno">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
